<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8"/>
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"/>
	<title>型号注册</title>
	<meta name="renderer" content="webkit"/>
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
	<meta name="apple-mobile-web-app-status-bar-style" content="black"/> 
	<meta name="apple-mobile-web-app-capable" content="yes"/>
	<meta name="format-detection" content="telephone=no"/>
	<link rel="stylesheet" href="../../../js/layui/css/layui.css"/>
	<link rel="stylesheet" href="../../../css/main.css"/>
	<link rel="stylesheet" href="../../../css/font-awesome.min.css"/>
	<script src="../../../js/layui/layui.js" charset="utf-8"></script>
</head>
<body class="childrenBody">
	<form id="s_form" class="layui-form >
		<blockquote class="layui-elem-quote news_search">
			<div class="layui-inline">
				
				<div class="layui-inline">
					设备分类:
				</div>
			    <div class="layui-input-inline">
			    	<select name="equipCateg" lay-filter="equipCateg" class="layui-select T-select" 
			    	T-options="url:'dict/all?dictType=equip_categ',value:'dictCode',name:'dictName',isShowSel:'false'">
					</select>
			    </div>
			    			    
			    <div class="layui-input-inline">
			    	<select name="equipType" id="equipType" class="layui-select">
					</select>
			    </div>
			    
			    <a href="javascript:void(0);" class="layui-btn search_btn" id="search">查询</a>
			    <div class="layui-inline">
					<a class="layui-btn layui-btn-normal T-click-layer-page" href="javascript:void(0);" T-options="url:'./model_add.html',title:'新增'">新增</a>
				</div>
			</div>
		</blockquote>
	
	</form>
	<div class="layui-form news_list">
		<table class="layui-table t-layui-table T-dtGrid" T-options="searchFormId:'s_form', searchBtn:'search',url:'model/page'">
			<colgroup>
				<col width="20%"/>
		    	<col width="20%"/>
		    	<col width="20%"/>
		    	<col width="20%"/>
			    <col width="20%"/>
			</colgroup>
			<thead>
			    <tr>
					<th>型号名称</th>
					<th>产品名称</th>
					<th>一级分类</th>
					<th>二级分类</th>
					<!--<th>通信速率</th>-->
					<th>操作</th>
			    </tr> 
			</thead>
			<tbody>
			</tbody>
		</table>
	</div>
		<script id="templ" type="text/x-jquery-tmpl">
		{{#  layui.each(d.data, function(index, item){ }}
		<tr>
			<td>{{item.fullName}}</td>
			<td>{{item.productName}}</td>
			<td>
			{{# if(item.equipCateg == "0101"){ }}
				终端设备
			{{# } }}
			{{# if(item.equipCateg == "0102"){ }}
				集抄设备
			{{# } }}
			{{# if(item.equipCateg == "0103"){ }}
				充值设备
			{{# } }}
			</td>	
			<td>
			{{# if(item.equipType == "0201"){ }}
				电表
			{{# } }}
			{{# if(item.equipType == "0202"){ }}
				水表
			{{# } }}
			{{# if(item.equipType == "0203"){ }}
				燃气表
			{{# } }}
			{{# if(item.equipType == "0203"){ }}
				热量表
			{{# } }}
			{{# if(item.equipType == "0211"){ }}
				采集器
			{{# } }}
			{{# if(item.equipType == "0212"){ }}
				集中器
			{{# } }}
			</td>
			<!--<td>{{item.baudRate}}</td>-->
			<td align="left">
            	<a class="layui-btn layui-btn-mini T-click-layer-page" T-options="url:'./model_detail.html',title:'型号注册详情',dataField:{modelId:'{{item.modelId}}'}">查看</a>				
				<a class="layui-btn layui-btn-danger layui-btn-mini T-click-prompt" T-options="url:'model/{{item.modelId}}',method:'DELETE',msg:'确定删除订单?'">删除</a>
			</td>
			<!--<td>
			{{# if(item.isRemoteControl == 0){ }}
				不支持
			{{# } }}
			{{# if(item.isRemoteControl == 1){ }}
				支持
			{{# } }}
			</td>
			<td>
			{{# if(item.isCostControl == 0){ }}
				不支持
			{{# } }}
			{{# if(item.isCostControl == 1){ }}
				支持
			{{# } }}
			</td>-->
		</tr>
		{{#  }) }}
	</script>
	
	<div class="page">
		<div class="paginations" id="pagination"></div>
	</div>
	</div>
	  <script src="./model_list.js" charset="utf-8"></script>
	<script>
		layui.config({
			base: '../../../js/' //你存放新模块的目录，注意，不是layui的模块目录
		}).use(['form', 'layer','T'], function(){
			
		})
	</script>
</body>
</html>