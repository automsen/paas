		<div class="s-main">
			<div class="">
				<!--表单区域-->
				<form class="layui-form" id="s-form">
					<div class="layui-form-item">
						<div class="layui-inline">
							<label class="layui-form-label">接入机构</label>
							<div class="layui-input-inline">
								<select class="left s-select" id="orgSelect" lay-filter="jigou"> </select>
							</div>
						</div>
						<div class="layui-inline">
							<label class="layui-form-label cus-layui-form-label">仪表名称</label>
							<div class="layui-input-inline">
								<select class="left s-select" lay-filter="meterSelect" id="meterSelect" lay-search=""> </select>
							</div>
						</div>

						<div class="layui-inline" id="btnDiv">
							<button type='button' class='layui-btn layui-btn-normal' id='readData' style="display:none">读数</button>
							<button type='button' class='layui-btn layui-btn-normal' id='refreshData' style="display:none">刷新</button>
						</div>

						<!--<div class="layui-inline">
							<a class="layui-btn layui-btn" id="elecDataExport" href="javascript:void(0);">电表数据导出</a>
							<a class="layui-btn layui-btn" id="waterDataExport" href="javascript:void(0);">水表数据导出</a>
						</div>-->
						
						<div class="row-one">
							<label class="left">设备数量：</label>
							<div class="left form-text" id="totalOrgEquipment">
							</div>
							<label class="left">接入时间：</label>
							<div class="left form-text" id="orgTime">
								--
							</div>
							<label class="left"></label>
							<div class="clearfix"></div>
						</div>
						<div class="row-one">
							<label class="left">仪表地址：</label>
							<div class="left form-text meterInfo" id="equipNumber">
							</div>
							<label class="left">仪表型号：</label>
							<div class="left form-text meterInfo" id="modelName">
							</div>
							<label class="left">通信方式：</label>
							<div class="left form-text meterInfo" id="netTypeCode">
							</div>
							<div class="clearfix"></div>
						</div>
						<div class="row-one elec">
							<label class="left">电表类型：</label>
							<div class="left form-text meterInfo" id="elecMeterTypeCode">
							</div>
							<label class="left">电压变比：</label>
							<div class="left form-text meterInfo" id="elecPt">
							</div>
							<label class="left">电流变比：</label>
							<div class="left form-text meterInfo" id="elecCt">
							</div>
							<label class="left"></label>
							<div class="left form-text">
							</div>
							<div class="clearfix"></div>
						</div>
						<div class="row-one" id="tableInfo">
							<label class="left">远程通断：</label>
							<div class="left form-text meterInfo" id="isRemoteControl">
							</div>
							<label class="left">通断状态：</label>
							<div class="left form-text readValue" id="isOff">
								<!-- <button type="button" class="btn-off">断开</button> -->
							</div>
							<label class="left">表计付费：</label>
							<div class="left form-text meterInfo" id="isCostControl">
							</div>
							<label class="left">仪表余额：</label>
							<div class="left form-text readValue" id="meterMoney">
							</div>
							<div class="clearfix"></div>
						</div>
						
						<div class="row-one" id="hotInfo">
						</div>
						
						<div class="row-one" id="hotInfo2">
						</div>
					</div>
					
				</form>
				<div class="elec">
					<!--三色表区域-->
					<div class="con-color">
						<div class="con-purple left">
							<div class="con-purple-one">
								组合有功总电能(kW·h)
							</div>
							<div class="con-purple-two readTime">
								--
							</div>
							<div class="con-purple-three readValue" id="totalActiveE">
								--
							</div>
							<div class="con-purple-four">
								尖：
								<span id="totalActiveE1" class="readValue">--</span> &nbsp; &nbsp;峰：
								<span id="totalActiveE2" class="readValue">--</span> &nbsp; &nbsp;平：
								<span id="totalActiveE3" class="readValue">--</span> &nbsp; &nbsp;谷：
								<span id="totalActiveE4" class="readValue">--</span>
							</div>
						</div>
						<div class="con-purple left con-orange">
							<div class="con-purple-one">
								正向有功总电能(kW·h)
							</div>
							<div class="con-purple-two readTime">
								--
							</div>
							<div class="con-purple-three readValue" id="positiveActiveE">
								--
							</div>
							<div class="con-purple-four">
								尖：
								<span id="positiveActiveE1" class="readValue">--</span> &nbsp; &nbsp;峰：
								<span id="positiveActiveE2" class="readValue">--</span> &nbsp; &nbsp;平：
								<span id="positiveActiveE3" class="readValue">--</span> &nbsp; &nbsp;谷：
								<span id="positiveActiveE4" class="readValue">--</span>
							</div>
						</div>
						<div class="con-purple left con-red">
							<div class="con-purple-one">
								反向有功总电能(kW·h)
							</div>
							<div class="con-purple-two readTime">
								--
							</div>
							<div class="con-purple-three readValue" id="reverseActiveE">
								--
							</div>
							<div class="con-purple-four">
								尖：
								<span id="reverseActiveE1" class="readValue">--</span> &nbsp; &nbsp;峰：
								<span id="reverseActiveE2" class="readValue">--</span> &nbsp; &nbsp;平：
								<span id="reverseActiveE3" class="readValue">--</span> &nbsp; &nbsp;谷：
								<span id="reverseActiveE4" class="readValue">--</span>
							</div>
						</div>
						<div class="clearfix"></div>
					</div>
					<!--三相区域-->
					<div class="con-sx">
						<div class="con-sx-time">
							<p class="left">最近读数时间：<span class="readTime">--</span></p>
							<div class="circle-purple left"></div>
							<p class="left">A相</p>
							<div class="circle-orange left"></div>
							<p class="left">B相</p>
							<div class="circle-red left"></div>
							<p class="left">C相</p>
							<div class="clearfix"></div>
						</div>
						<div class="con-sx-meter">
							<div class="meter-one left">
								<h2>有功功率（kW）</h2>
								<div class="meter-one2" style="font-size: 24px">
									总：
									<span id="activeP" class="readValue">--</span>
								</div>
								<div class="meter-one2">
									A相：
									<span id="activePa" class="readValue">--</span> &nbsp; &nbsp;B相：
									<span id="activePb" class="readValue">--</span> &nbsp; &nbsp;C相：
									<span id="activePc" class="readValue">--</span>
								</div>
							</div>
							<div class="meter-one left">
								<h2>无功功率（kVar）</h2>
								<div class="meter-one2" style="font-size: 24px">
									总：
									<span id="reactiveP" class="readValue">--</span>
								</div>
								<div class="meter-one2">
									A相：
									<span id="reactivePa" class="readValue">--</span> &nbsp; &nbsp;B相：
									<span id="reactivePb" class="readValue">--</span> &nbsp; &nbsp;C相：
									<span id="reactivePc" class="readValue">--</span>
								</div>
							</div>
							<div class="meter-one left">
								<h2>视在功率（kVA）</h2>
								<div class="meter-one2" style="font-size: 24px">
									总：
									<span class="readValue">--</span>
								</div>
								<div class="meter-one2">
									A相：
									<span class="readValue">--</span> &nbsp; &nbsp;B相：
									<span class="readValue">--</span> &nbsp; &nbsp;C相：
									<span class="readValue">--</span>
								</div>
							</div>
							<div class="meter-one left">
								<h2>功率因数</h2>
								<div class="meter-one2" style="font-size: 24px">
									总：
									<span id="PF" class="readValue">--</span>
								</div>
								<div class="meter-one2">
									A相：
									<span id="PFa" class="readValue">--</span> &nbsp; &nbsp;B相：
									<span id="PFb" class="readValue">--</span> &nbsp; &nbsp;C相：
									<span id="PFc" class="readValue">--</span>
								</div>
							</div>
							<div class="clearfix"></div>
						</div>
					</div>
					<!--表格-->
					<table class="layui-table t-layui-table  s-table" t-options="">
						<colgroup>
							<col width="20%" />
							<col width="20%" />
							<col width="20%" />
							<col width="20%" />
							<col width="20%" />
						</colgroup>
						<thead>
							<tr>
								<th>数据项</th>
								<th>A相</th>
								<th>B相</th>
								<th>C相</th>
								<th>最近读数时间</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td>电压（V）</td>
								<td id="Ua" class="readValue">--</td>
								<td id="Ub" class="readValue">--</td>
								<td id="Uc" class="readValue">--</td>
								<td class="readTime">--</td>
							</tr>
							<tr>
								<td>电流（A）</td>
								<td id="Ia" class="readValue">--</td>
								<td id="Ib" class="readValue">--</td>
								<td id="Ic" class="readValue">--</td>
								<td class="readTime">--</td>
							</tr>
						</tbody>
					</table>
				</div>
				
				<div class="water">
					<div class="con-color">
						<div class="con-purple left " style="visibility:hidden">
							<!-- 占位 -->
						</div>
						<div class="con-purple left">
							<div class="con-purple-one">
								当前止码(m³)
							</div>

							<div class="con-purple-three readValue" id="waterFlow">
								--
							</div>
							<div class="con-purple-two readTime">
								--
							</div>
						</div>
					</div>
				</div>
				<div class="gas">
					<div class="con-color">
						<div class="con-purple left " style="visibility:hidden">
							<!-- 占位 -->
						</div>
						<div class="con-purple left">
							<div class="con-purple-one">
								当前止码(m³)
							</div>

							<div class="con-purple-three readValue" id="waterFlow_two">
								--
							</div>
							<div class="con-purple-two readTime">
								--
							</div>
						</div>
					</div>
				</div>
				
				<div class="hot">
					<div class="con-color">
						<div class="con-purple left">
							<div class="con-purple-one">
								电表电量(kW·h)
							</div>
							<div class="con-purple-two readTime">
								--
							</div>
							<div class="con-purple-three readValue" id="TAE">
								--
							</div>
							<div class="con-purple-four">
								--
							</div>
						</div>
						
						<div class="con-purple left con-red">
							<div class="con-purple-one">
								 室内温度
							</div>
							<div class="con-purple-two readTime">
								--
							</div>
							<div class="con-purple-three readValue" id="IDT">
								--
							</div>
							<div class="con-purple-four">
								 室外：
								<span id="ODT" class="readValue">--</span> &nbsp; &nbsp;设定：
								<span id="STT" class="readValue">--</span> &nbsp; &nbsp;入水：
								<span id="IWT" class="readValue">--</span> &nbsp; &nbsp;出水：
								<span id="OWT" class="readValue">--</span>
							</div>
						</div>
						
						<div class="con-purple left con-orange">
							<div class="con-purple-one">
								水流量(m³)
							</div>
							<div class="con-purple-two readTime">
								--
							</div>
							<div class="con-purple-three readValue" id="WF">
								--
							</div>
							<div class="con-purple-four">
								--
							</div>
						</div>
						
						<div class="clearfix"></div>
					</div>
				</div>
			</div>
		</div>

		<div class="right-item">
			<!--<div class="con_icon con_icon1" id="hotBtn">
				<a href="#" title="热"><img src="../../images/icon_r4.png" width="40" height="40" /></a>
			</div>-->
		</div>
		
		<script id="hotTempl" type="text/x-jquery-tmpl">
			<label class="left">工作模式：</label>
			<div class="left form-text" T-name="workingMode">--
			</div>
			
			<label class="left">功能位：</label>
			<div class="left form-text" T-name="functionDigit">--</div>
			
			<label class="left">波特率：</label>
			<div class="left form-text" T-name="rfBaudRate">--</div>
			
			<label class="left">心跳间隔：</label>
			<div class="left form-text" T-name="beattim">--</div>
			
			<div class="clearfix"></div>
		</script>
		<script id="hotTempl2" type="text/x-jquery-tmpl">
			<label class="left">设备数量：</label>
			<div class="left form-text" T-name="childrenNum">--</div>
			
			<label class="left">上报间隔：</label>
			<div class="left form-text" T-name="reportingInterval">--</div>	
		</script>
	</body>
</html>