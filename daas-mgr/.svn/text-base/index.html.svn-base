		<script type="application/javascript">
			<!-- js拦截-->
			// 需要验证token 
			var token = localStorage.getItem("_twps_qy");
			if(token == null || token == '') {
				if(parent != null) {
					window.parent.location.href = "login.html";
				} else {
					window.location.href = "login.html";
				}
			}

			function exitUser() {
				layui.use(['jquery'], function() {
					// 弹出消息窗确定
					var layer = layui.layer;

					var isloginMsg = layui.i18n.getMessage("isLoginOut") || " 是否退出系统？";

					var isTrue = layui.i18n.getMessage("isTrue") || "是";

					var isFalse = layui.i18n.getMessage("isFalse") || '否';
					//询问框
					layer.confirm(isloginMsg, {
						btn: [isTrue, isFalse] //按钮
					}, function() {
						localStorage.removeItem("_twps_qy");
						localStorage.removeItem("_twps_qy_d");
						localStorage.removeItem("_twps_qy_refresh");
						window.location.href = "/login.html";
					}, function() {});
				})
			}
		</script>
		<script src="js/layui/layui.js" charset="utf-8"></script>
